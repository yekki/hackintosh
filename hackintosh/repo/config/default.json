        ],
        "os-x-acpi-battery-driver": [
          "ACPIBatteryManager.kext"
        ],
        "os-x-voodoo-ps2-controller": [
          "VoodooPS2Controller.kext",
          "VoodooPS2Daemon",
          "org.rehabman.voodoo.driver.Daemon.plist"
        ],
        "os-x-usb-inject-all": [
          "USBInjectAll.kext"
        ],
        "os-x-intel-network": [
          "IntelMausiEthernet.kext"
        ]
      },
      "clover": {
        "kexts_to_patch": [
        {
          "Name": "com.apple.iokit.IOAHCIBlockStorage",
          "Find": "AEFQUExFIFNTRAA=",
          "Replace": "AAAAAAAAAAAAAAA=",
          "Comment": "Enable TRIM for SSD"
        }
        ]
      }
    },
    "brightness_control": {
      "kext": {
        "AppleBacklightInjector": [
          "AppleBacklightInjector.kext"
        ]
      },
      "clover": {
        "kexts_to_patch": [
          {
            "Name": "com.apple.driver.AppleBacklight",
            "Find": "RiV1VCUwNHgA",
            "Replace": "RiV1VHh4eHgA",
            "Comment": "change F%uT%04x to F%uTxxxx in AppleBacklightInjector.kext (credit RehabMan)"
          }
        ]
      }
    }
  }
